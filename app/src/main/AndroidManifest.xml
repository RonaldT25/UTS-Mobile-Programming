<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homepc.Ezyfoody">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_launcher_foreground"
        android:label="Ezy foody"
        android:roundIcon="@mipmap/logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.homepc.Ezyfoody.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.homepc.Ezyfoody.LoginOptionsPage" />
        <activity android:name="com.example.homepc.Ezyfoody.SignInPage" />
        <activity android:name="com.example.homepc.Ezyfoody.SignUpPage" />
        <activity
            android:name="com.example.homepc.Ezyfoody.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity android:name="com.example.homepc.Ezyfoody.OrderPage"></activity>
    </application>


</manifest>